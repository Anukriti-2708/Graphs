class Solution {
    public boolean canVisitAllRooms(List<List<Integer>> rooms) {
        int n = rooms.size();
        boolean[] b = new boolean[n];
       
        dfs(rooms,0,b);
         
        
       for(boolean flag : b)
        if(flag != true)
            return false;
    
    return true;
    }
    
    public void dfs(List<List<Integer>> rooms,int node, boolean[]b){
        b[node] = true;
        for(Integer it : rooms.get(node)){
            if(b[it] != true)
               dfs(rooms,it,b);
            
        }
    }
}


